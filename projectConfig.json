{
    "path": {
        "src": {
            "srcPath": "./src",
            "components": "/_components",
            "html": [
                "/**/*.html"
            ],
            "style": [
                "/assets/styles/libs/*.*",
                "/assets/styles/*.*"
            ],
            "script": [
                "/assets/js/*.js"
            ],
            "libs": [
                "/assets/libs/*.*"
            ],
            "image": [
                "/assets/img/**/*.*"
            ],
            "font": [
                "/assets/fonts/**/*.*"
            ]
        },
        "dist": {
            "distPath": "./dist",
            "html": "/",
            "style": "/assets/css/",
            "script": "/assets/js/",
            "libs": "/assets/libs/",
            "image": "/assets/img/",
            "font": "/assets/fonts/"
        }
    }
}
